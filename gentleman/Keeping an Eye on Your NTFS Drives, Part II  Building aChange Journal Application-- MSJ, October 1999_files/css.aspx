.mnpMenuTop
{
	border-style: solid; 
	border-color: #999999; 
	border-width: 0px 1px 0px 0px;
	padding-top: 2px;
	background: #F1F1F1;
	font-size: small;
}

.mnpMenuSub
{
	position: absolute; 
	border: solid 1px #999999; 
	background: #F1F1F1;
	padding: 2px 0px;
}

.mnpMenuRow
{
	cursor: hand;
	padding: 1px 15px 3px 6px;
	border-style: solid;
	border-width: 1px;
	margin: 0px 2px;
	color: Black;
}

.mnpMenuRow a
{
	text-decoration: none; 
	color: black;
	background: inherit;
	font-weight: normal;
}

.mnpMenuRow a:visited
{
	text-decoration: none; 
	color: black;
	background: inherit;
	font-weight: normal;
}

.mnpMenuRow a:hover
{
	text-decoration: none;
	color: Black;
	background: inherit;
	font-weight: normal;
}

.mnpMenuLabel
{
	padding: 2px 2px 4px 4px;
	background: #F1F1F1;
	color: black;
}

.mnpMenuBorder
{
	height: 1px;
	background: #999999;
	overflow: hidden;
	margin: 2px 0px;
}

.mnpMenuArrow
{
	position: absolute;
	margin-top: 4px;
}

.mnpMenuScroller
{
	text-align: center;
	height: 19px;
	border-style: solid;
	border-width: 1px;
	padding-top: 4px;
	margin: 0px 2px;
	overflow: hidden;
}

.mnpMenuScrollArea
{
	overflow: hidden;
	background: inherit;
}

